
<%- include('header') -%>

    <div class="box" id="heading">
        <h1> <%= listType %> </h1>
    </div>

    <div class="box">

        <% newItem.forEach((eachItem)=>{ %>
            <div class="item">
                <input type="checkbox">
                <p><%= eachItem.name %></p>
            </div>
        <% }) %>
        
            <form action="/" method="post">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value="<%= listType %>">+</button>
            </form>
    </div>

<%- include('footer') -%>